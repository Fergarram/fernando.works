{
	"name": "fernando-works",
	"version": "6.6.6",
	"description": "My personal site",
	"main": "./src/build.js",
	"scripts": {
		"start": "npm run dev",
		"styles": "tailwindcss-cli -w build -i ./tailwind.css -o ./assets/style.css",
		"build": "node ./src/cleanPublic.js && npm run build:rollup && npm run build:html",
		"build:rollup": "rollup -c",
		"build:html": "node ./src/build.js",
		"dev": "rollup  -c -w --no-watch.clearScreen",
		"serve": "node ./src/cleanPublic.js && npm run build:rollup && NODE_ENV=production node ./src/server.js",
		"esbuild": "node esbuild"
	},
	"type": "commonjs",
	"engines": {
		"node": ">= 12.0.0"
	},
	"dependencies": {
		"@elderjs/elderjs": "^1.5.5",
		"@elderjs/plugin-browser-reload": "^0.1.37",
		"@elderjs/plugin-markdown": "^0.1.37",
		"@elderjs/plugin-seo-check": "0.1.37",
		"body-parser": "^1.19.0",
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"del": "^6.0.0",
		"dotenv": "^8.2.0",
		"fs-extra": "^9.1.0",
		"glob": "^7.1.6",
		"intersection-observer": "^0.12.0",
		"polka": "^0.5.2",
		"sirv": "^1.0.11",
		"svelte": "^3.38.3",
		"svelte-preprocess": "^4.6.9"
	},
	"devDependencies": {
		"@babel/core": "^7.13.10",
		"@babel/plugin-syntax-dynamic-import": "^7.8.3",
		"@babel/plugin-transform-runtime": "^7.13.10",
		"@babel/preset-env": "^7.13.10",
		"@babel/runtime": "^7.13.10",
		"autoprefixer": "^10.2.5",
		"nodemon": "^2.0.7",
		"postcss": "^8.2.8",
		"rollup": "^2.41.1"
	},
	"author": "Fernando Garcia",
	"license": "MIT",
	"files": [
		"index.js",
		"elderjs"
	]
}
